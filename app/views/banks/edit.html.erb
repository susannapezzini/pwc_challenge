<div class="form_container">
<h2 class="h2">Edit <%= @bank.name %></h2>
    <%= simple_form_for @bank do |f| %>
      <%= f.input :name  %>
      <%#= f.association :website %>
      <%= f.input :address  %>
      <%= f.country_select :country, prompt: 'Select Country', class: 'form-control string optional' %>
      <br>
      <%= f.submit class: 'btn' %>
    <% end %>
</div>